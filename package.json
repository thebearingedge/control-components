{
  "name": "controlled-components",
  "version": "1.0.0-alpha.3",
  "description": "React form control system.",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "files": [
    "README.md",
    "LICENSE",
    "umd/",
    "cjs/",
    "es/"
  ],
  "scripts": {
    "lint": "eslint src/",
    "test": "cd src/ && mocha -r babel-register '*.test.js'",
    "tdd": "npm -s t -- -w -R min",
    "cover": "nyc npm -s t",
    "report": "codecov",
    "build": "babel-node build.js",
    "watch:pug": "pug demo/ -o demo/ -P -w",
    "watch:js": "watchify demo/ -vd -t babelify -o demo/main.js",
    "watch:sync": "browser-sync start -c sync.js",
    "demo": "run-p -s watch:*",
    "travis": "run-s -s lint cover build docs:build",
    "precommit": "run-s -s lint test",
    "prepublishOnly": "run-s -s travis",
    "docs:clean": "rimraf _book/",
    "docs:build": "gitbook build",
    "docs:watch": "gitbook serve",
    "docs:publish": "./docs.bash"
  },
  "keywords": [],
  "author": "Tim Davis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebearingedge/controlled-components.git"
  },
  "bugs": {
    "url": "https://github.com/thebearingedge/controlled-components/issues"
  },
  "homepage": "https://github.com/thebearingedge/controlled-components#readme",
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "babelify": "^8.0.0",
    "browser-sync": "^2.18.13",
    "browserify": "^14.5.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "codecov": "^3.0.0",
    "deep-freeze": "0.0.1",
    "dotenv": "^4.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.12.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "gitbook-cli": "^2.3.2",
    "gitbook-plugin-collapsible-menu": "^1.0.3",
    "gitbook-plugin-github": "^2.0.0",
    "gzip-size": "^4.1.0",
    "husky": "^0.14.3",
    "jsdom": "^11.5.1",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.0",
    "pretty-bytes": "^4.0.2",
    "pug-cli": "^1.0.0-alpha6",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-uglify": "^2.0.1",
    "sinon": "^4.1.1",
    "sinon-chai": "^2.14.0",
    "watchify": "^3.9.0"
  }
}
