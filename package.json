{
  "name": "controlled-components",
  "version": "1.0.0-alpha.1",
  "description": "Streamlined form controls for React.",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint src/",
    "test": "mocha -r babel-register src/*.test.js",
    "tdd": "npm -s t -- -w -R min",
    "cover": "nyc npm -s t",
    "report": "codecov",
    "build:dist": "babel src/ -d dist/ --ignore test.js,__test__.js",
    "prebuild:dist": "rimraf dist/ && mkdirp dist/",
    "build:pug": "pug src/demo/ -o demo/ -P",
    "build:js": "browserify src/demo/ -t babelify -o demo/main.js",
    "build:demo": "run-p -s build:pug build:js",
    "build": "run-s -s build:dist build:demo",
    "prebuild:demo": "rimraf demo/ && mkdirp demo/",
    "watch:pug": "run-s -s 'build:pug -- -w'",
    "watch:js": "watchify src/demo/ -vd -t babelify -o demo/main.js",
    "watch:sync": "browser-sync start -c sync.js",
    "demo": "run-p -s watch:*",
    "predemo": "run-s -s build:demo",
    "travis": "run-s -s lint cover build",
    "precommit": "run-s -s lint test",
    "prepublishOnly": "run-s -s travis"
  },
  "keywords": [],
  "author": "Tim Davis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebearingedge/control-components.git"
  },
  "bugs": {
    "url": "https://github.com/thebearingedge/control-components/issues"
  },
  "homepage": "https://github.com/thebearingedge/control-components#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelify": "^8.0.0",
    "browser-sync": "^2.18.13",
    "browserify": "^14.5.0",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "codecov": "^3.0.0",
    "deep-freeze": "0.0.1",
    "dotenv": "^4.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.12.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jsdom": "^11.4.0",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.3.0",
    "pug-cli": "^1.0.0-alpha6",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "rimraf": "^2.6.2",
    "sinon": "^4.1.1",
    "sinon-chai": "^2.14.0",
    "watchify": "^3.9.0"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "dependencies": {
    "prop-types": "^15.5.7"
  }
}
